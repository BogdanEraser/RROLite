    <top>
        <Pane prefHeight="80.0" BorderPane.alignment="CENTER">
            <children>
                <ImageView fitHeight="80.0" fitWidth="264.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../images/logo_color.png"/>
